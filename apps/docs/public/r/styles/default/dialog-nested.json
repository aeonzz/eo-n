      "content": "import React from \"react\";\r\n\r\nimport { Button } from \"@/registry/default/ui/button\";\r\nimport {\r\n  Dialog,\r\n  DialogContent,\r\n  DialogDescription,\r\n  DialogFooter,\r\n  DialogHeader,\r\n  DialogTitle,\r\n  DialogTrigger,\r\n} from \"@/registry/default/ui/dialog\";\r\n\r\nexport default function DialogNested() {\r\n  return (\r\n    <Dialog>\r\n      <DialogTrigger\r\n        render={<Button variant=\"outline\">Open Settings</Button>}\r\n      />\r\n      <DialogContent className=\"sm:max-w-[425px]\">\r\n        <DialogHeader>\r\n          <DialogTitle>Account Settings</DialogTitle>\r\n          <DialogDescription>\r\n            Manage your account preferences and settings.\r\n          </DialogDescription>\r\n        </DialogHeader>\r\n        <div className=\"py-4\">\r\n          <p>Your changes will be automatically saved.</p>\r\n          <div className=\"grid gap-2 py-2\">\r\n            <div className=\"flex items-center justify-between\">\r\n              <span className=\"font-medium\">Email notifications</span>\r\n              <span className=\"text-sm text-gray-500\">Enabled</span>\r\n            </div>\r\n            <div className=\"flex items-center justify-between\">\r\n              <span className=\"font-medium\">Account status</span>\r\n              <span className=\"text-sm text-gray-500\">Active</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <DialogFooter className=\"flex justify-between\">\r\n          <Dialog>\r\n            <DialogTrigger\r\n              render={<Button variant=\"ghost\">Advanced Options</Button>}\r\n            />\r\n            <DialogContent className=\"sm:max-w-[425px]\">\r\n              <DialogHeader>\r\n                <DialogTitle>Advanced Settings</DialogTitle>\r\n                <DialogDescription>\r\n                  Configure advanced options for your account. These settings\r\n                  are for experienced users.\r\n                </DialogDescription>\r\n              </DialogHeader>\r\n              <div className=\"py-4\">\r\n                <div className=\"grid gap-3\">\r\n                  <div className=\"flex items-center justify-between\">\r\n                    <span className=\"font-medium\">Developer mode</span>\r\n                    <span className=\"text-sm text-gray-500\">Disabled</span>\r\n                  </div>\r\n                  <div className=\"flex items-center justify-between\">\r\n                    <span className=\"font-medium\">API access</span>\r\n                    <span className=\"text-sm text-gray-500\">Restricted</span>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <DialogFooter>\r\n                <Button>Save Changes</Button>\r\n              </DialogFooter>\r\n            </DialogContent>\r\n          </Dialog>\r\n          <Button type=\"submit\">Save Changes</Button>\r\n        </DialogFooter>\r\n      </DialogContent>\r\n    </Dialog>\r\n  );\r\n}\r\n",
